#One or more words that identify this distribution:
DISTRO_NAME='TazPuppy'
#version number of this distribution:
DISTRO_VERSION=5.0
#The distro whose binary packages were used to build this distribution:
DISTRO_BINARY_COMPAT='slitaz'
#Prefix for some filenames: exs: slackosave.2fs, slacko-6.3.2.sfs
DISTRO_FILE_PREFIX='tazpup'
#The version of the distro whose binary packages were used to build this distro:
DISTRO_COMPAT_VERSION='5.0'
#the kernel pet package used:
DISTRO_KERNEL_PET='Huge_Kernel'
DISTRO_TARGETARCH='x86'
DISTRO_XORG_AUTO='yes'
DISTRO_DB_SUBNAME='slitaz5.0'
BUILD_FROM_WOOF='testing;9b645cc;2016-06-21 13:53:34 +1000'
#32-byte alpha-numeric ID-string appended to vmlinuz, puppy_slacko_6.3.2.sfs, zdrv_slacko_6.3.2.sfs and devx.sfs:
DISTRO_IDSTRING='x171122003801ZZZZ5.0XXXXXXXXXXXX'
#Puppy default filenames...
#Note, the 'SFS' files below are what the 'init' script in initrd.gz searches for,
#for the partition, path and actual files loaded, see PUPSFS and ZDRV in /etc/rc.d/PUPSTATE
DISTRO_PUPPYSFS='puppy_tazpup_5.0.sfs'
DISTRO_ZDRVSFS='zdrv_tazpup_5.0.sfs'
DISTRO_FDRVSFS='fdrv_tazpup_5.0.sfs'
DISTRO_ADRVSFS='adrv_tazpup_5.0.sfs'
DISTRO_YDRVSFS='ydrv_tazpup_5.0.sfs'
DISTRO_PUPPYDATE='Aug 2018'
#multiarch distros, such as Ubuntu, will have this. ex: /usr/lib/i386-linux-gnu, so DISTRO_ARCHDIR=i386-linux-gnu
DISTRO_ARCHDIR=''
